{{#each list}}
    <a data-id="{{this.id}}" data-name="{{this.name}}" class='list{{this.id}} toggle'><p>
      {{this.name}}
      <i class="material-icons add_task" name:"button">note_add</i>
      <i class="material-icons update" name:"button">mode_edit</i>
      <i class="material-icons delete" name:"button">delete</i>
</p>
      </a>
<ul class="inner">
  {{#each this.tasks}}
      <li data-id="{{this.id}}"><p>{{this.name}}
        <i class="material-icons delete_task" name:"button">delete</i>
      </p>
      </li>
      {{/each}}
    </ul>
{{/each}}

































